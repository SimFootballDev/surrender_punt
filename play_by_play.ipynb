    "    idBar = tqdm(total=len(idList),desc='id')\n",
    "    for i in idList:\n",
    "        idBar.set_description(i)\n",
    "        allData.append(getGameData(s,i))\n",
    "        idBar.update(1)\n",
    "    sBar.update(1)"
